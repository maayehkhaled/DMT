            <class name="com.qpros.test.activation.ManageOffersManagement"/>
            <class name="com.qpros.test.activation.PEPScenario"/>
        </classes>
    </test>


    <test name="RMI Process">
        <classes>
            <class name="com.qpros.test.rmi.RMIAppeal"/>
            <class name="com.qpros.test.rmi.RMIBCOC"/>
            <class name="com.qpros.test.rmi.RMIFirstAssessment"/>
            <class name="com.qpros.test.rmi.RMIReassessment"/>
        </classes>
    </test>
    <test name="BCOC Approval Process">
        <classes>
            <class name="com.qpros.test.bcoc.BCOCApprove"/>
            <class name="com.qpros.test.bcoc.BCOCReject"/>
        </classes>
    </test>

    <test name="approve/Payment Scenario">
        <classes>

            <class name="com.qpros.test.agent.ApproveApplication"/>
            <class name="com.qpros.test.agent.RejectApplication"/>
            <class name="com.qpros.test.agent.ReassessmentApprove"/>
            <class name="com.qpros.test.acoc.AcocTrigger"/>
            <class name="com.qpros.test.payments.AssessmentPayment">
                <methods>
                    <exclude name="newSchedulesNewCard"/>
                    <exclude
                            name="updatePaymentsInstructions"/>
                    <exclude name="blockCard"/>
                </methods>
            </class>
            <class name="com.qpros.test.activation.AddPartner"/>
        </classes>
    </test>

