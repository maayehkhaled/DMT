            <class name="com.qpros.test.agent.ReassessmentReject"/>
            <class name="com.qpros.test.agent.RejectApplication"/>
        </classes>
    </test>

    <test name="ACOC Approval Process">
        <classes>
            <class name="com.qpros.test.acoc.ACOCApprove"/>
            <class name="com.qpros.test.acoc.AcocTrigger"/>
        </classes>
    </test>

    <test name="Activation Process">
        <classes>
            <class name="com.qpros.test.activation.ActivationOpportunities"/>
            <class name="com.qpros.test.activation.AddPartner"/>
