<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name=" SSA Test's">

    <listeners>
        <listener class-name="com.qpros.common.LogManager"/>
    </listeners>

    <test name="Agent Approval Process">
        <classes>
            <class name="com.qpros.test.agent.AppealApprove"/>
            <class name="com.qpros.test.agent.AppealReject"/>
            <class name="com.qpros.test.agent.ApproveApplication"/>
            <class name="com.qpros.test.agent.ReassessmentApprove"/>
